/**
 * @module @nhtio/eslint-config/plugins
 */
import type { FlatConfig } from '@typescript-eslint/utils/ts-eslint';
/**
 * The `eslint-plugin-unicorn` plugin
 * @see [eslint-plugin-unicorn](https://www.npmjs.com/package/eslint-plugin-unicorn) on NPM
 */
export declare const unicorn: FlatConfig.Plugin;
/**
 * The `@stylistic/eslint-plugin-ts` plugin
 * @remarks TypeScript stylistic rules for ESLint, migrated from `typescript-eslint`
 * @see [@stylistic/eslint-plugin-ts](https://www.npmjs.com/package/@stylistic/eslint-plugin-ts) on NPM
 */
export declare const stylistic: FlatConfig.Plugin;
/**
 * The `eslint-plugin-prettier` plugin
 * @see [eslint-plugin-prettier](https://www.npmjs.com/package/eslint-plugin-prettier) on NPM
 */
export declare const prettier: FlatConfig.Plugin;
/**
 * The `typescript-eslint` plugin
 * @see [typescript-eslint](https://www.npmjs.com/package/typescript-eslint) on NPM
 */
export declare const tseslint: FlatConfig.Plugin;
/**
 * The `eslint-plugin-n` plugin
 * @see [eslint-plugin-n](https://www.npmjs.com/package/eslint-plugin-n) on NPM
 */
export declare const node: FlatConfig.Plugin;
/**
 * The `@eslint/json` plugin
 * @see [@eslint/json](https://www.npmjs.com/package/@eslint/json) on NPM
 */
export declare const json: FlatConfig.Plugin;
/**
 * The `@eslint/markdown` plugin
 * @see [@eslint/markdown](https://www.npmjs.com/package/@eslint/markdown) on NPM
 */
export declare const markdown: FlatConfig.Plugin;
export { default as nht } from './nht';
